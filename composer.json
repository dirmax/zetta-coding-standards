{
    "name": "dirmax/zetta-coding-standard",
    "type": "phpcodesniffer-standard",
    "description": "CodeSniffer ruleset for the ZettaCMS coding standard",
    "keywords": ["ZettaCms", "coding standard", "phpcs"],
    "require": {
        "squizlabs/php_codesniffer": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Zetta\\": ["src/"]
        }
    }
}
